<?xml version="1.0" standalone="yes"?>
<xs:schema targetNamespace="http://token.petrinetwork.hu" 
	xmlns:mstns="http://petrinetwork.hu" 
	xmlns="http://token.petrinetwork.hu" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
	attributeFormDefault="qualified" 
	elementFormDefault="qualified" 
	xmlns:c="http://color.petrinetwork.hu" 
	xmlns:bi="http://baseitem.petrinetwork.hu" >
  <xs:import namespace="http://color.petrinetwork.hu" schemaLocation="petrinetwork.color.xsd" />
  <xs:import namespace="http://baseitem.petrinetwork.hu" schemaLocation="petrinetwork.baseitem.xsd" />
  <xs:element name="Token" msdata:Prefix="tok">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="c:TokenColor" />
      </xs:sequence>
      <xs:attribute ref="bi:name" />
      <xs:attribute ref="bi:unid" />
      <xs:attribute ref="bi:showannotation" />
    </xs:complexType>
  </xs:element>
</xs:schema>