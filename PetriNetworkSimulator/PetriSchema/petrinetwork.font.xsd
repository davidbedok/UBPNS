<?xml version="1.0" standalone="yes"?>
<xs:schema targetNamespace="http://font.petrinetwork.hu" 
	xmlns:mstns="http://petrinetwork.hu" 
	xmlns="http://font.petrinetwork.hu" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
	attributeFormDefault="qualified" 
	elementFormDefault="qualified" 
	xmlns:fs="http://fontstyle.petrinetwork.hu" >
  <xs:import namespace="http://fontstyle.petrinetwork.hu" schemaLocation="petrinetwork.fontstyle.xsd" />
  <xs:element name="DefaultFont" msdata:Prefix="f">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="FontName" msdata:Prefix="f" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
        <xs:element name="FontSize" msdata:Prefix="f" type="xs:float" minOccurs="0" msdata:Ordinal="1" />
      </xs:sequence>
      <xs:attribute ref="fs:bold" />
      <xs:attribute ref="fs:italic" />
      <xs:attribute ref="fs:underline" />
      <xs:attribute ref="fs:strikeout" />
    </xs:complexType>
  </xs:element>
</xs:schema>